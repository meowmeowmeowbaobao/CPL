#include <iostream>
using namespace std;
int main(){
	int n=0,len=0;
	char **a;
	cin>>n;
	a = new char*[n];
	for (int i=0;i<n;i++){
		cin>>len;
		a[i] = new char[len];
		cin>>a[i];
	}
	//for (int i=0;i<n;i++) cout<<a[i]<<endl;
	//int num=0; //計數器 
	char alp[26]={'a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z'};
	int  AlpNum[26]={0};
	
	for (int i=0;i<n;i++){
		for (int j=0;j<50;j++){
			for (int k=0;k<26;k++){
				if (a[i][j]==alp[k]) AlpNum[k] +=1;
			}
		}
		for (int k=0;k<26;k++) cout<<AlpNum[i];	
	}
}
