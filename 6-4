#include <iostream>
using namespace std;

int main() {
	int num=0,n=0;
	cin>>num;
	char **name=NULL,**typed=NULL;
	name = new char*[num];
	typed = new char*[num];
	
	for (int i=0;i<num;i++){
		cin>>n; //名字長度 
		name[i] = new char[n];
		for (int j=0;j<n;j++) cin>>name[i][j];
	}
	
	for (int i=0;i<num;i++){
		cin>>n; //名字長度 
		typed[i] = new char[n];
		for (int j=0;j<n;j++) cin>>typed[i][j];
	}
	
	//for (int i=0;i<num;i++) cout<<name[i]<<endl;
	//思考怎麼判斷
	int a1=0,a2=0;
	for (int i=0;i<num;i++){
		
		if(name[i][a1]==' '||typed[i][a2]==' ') break;
		else{
			if (name[i][a1]==typed[i][a2]){
				a1+=1;
				a2+=1;
			}
			else if(typed[i][a2]==typed[i][a2-1]) a2+=1;
			
		}
		
	
		
	} 
	
	return 0;
}
