}

void print_formula(CinParser ADD)
{
	if (ADD.base!='D' || ADD.base!='O' || ADD.base!='H') cout<<"Base number error!";
	else{
		
		
	}
}
//TEMPLATE END

//APPEND BEGIN
int main()
{
	CinParser ADD;
	cin.getline(ADD.input, LEN_MAX);
	cin >> ADD.base;

	// counting how many numbers
	ADD.num = count_num(ADD);
  	ADD.term = new long long int[ADD.num];

	// calculating the addition with manipulation of string
	str2ll(ADD);
	
	// print the equation
	print_formula(ADD);
  	delete[] ADD.term;
}
