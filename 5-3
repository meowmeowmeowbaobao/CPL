//PREPEND BEGIN
#include <iostream>
#include <iomanip>
using namespace std;
int main() 
{
	int i, j;
	int A[3][3];
	double A_inv[3][3] = {0};
	for (i = 0; i < 3; i++) 
	{
		for (j = 0; j < 3; j++) 
		{
			cin >> A[i][j];
                }
        }
//PREPEND END
    
//TEMPLATE BEGIN    
	double det_A;
	det_A = A[0][0]*(A[2][2]*A[1][1]-A[1][2]*A[2][1]) -A[0][1]*(A[1][0]*A[2][2]-A[2][0]*A[1][2]) +A[0][2]*(A[1][0]*A[2][1]-A[1][1]*A[2][0]);
	
	A_inv[0][0] = ((A[1][1]*A[2][2]-A[1][2]*A[2][1]))/det_A;
	A_inv[1][0] = -1*((A[0][1]*A[2][2]-A[0][2]*A[2][1]))/det_A;
	A_inv[2][0] = ((A[0][1]*A[1][2]-A[0][2]*A[1][1]))/det_A;
	
	A_inv[0][1] = -1*((A[1][0]*A[2][2]-A[1][2]*A[2][0]))/det_A;
	A_inv[1][1] = ((A[0][0]*A[2][2]-A[0][2]*A[2][0]))/det_A;
	A_inv[2][1] = -1*((A[0][0]*A[1][2]-A[0][2]*A[1][0]))/det_A;
	
	A_inv[0][2] = ((A[1][0]*A[2][1]-A[1][1]*A[2][0]))/det_A;
	A_inv[1][2] = -1*((A[0][0]*A[2][1]-A[0][1]*A[2][0]))/det_A;
	A_inv[2][2] = ((A[0][0]*A[1][1]-A[0][1]*A[1][0]))/det_A;
	
//TEMPLATE END    
    
//APPEND BEGIN  
        for (i = 0; i < 3; i++) 
    	{
    		for (j = 0; j < 3; j++) 
    		{
    			cout << fixed << setprecision(3) << A_inv[i][j] << '\t';
                }
                cout << endl;
        }
	return 0;
}
//APPEND END
