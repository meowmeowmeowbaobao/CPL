//PREPEND BEGIN
#include <iostream>
using namespace std;
int main()
{  
//PREPEND END

//TEMPLATE BEGIN
	char cc[20],temp[90]={};
	int num,vowel=0,consonant=0,k=0; //num代表那個字母要出現幾次 .k代表temp填到多少 
	cin.getline(cc,20);
	int size = 0;
	for (int i = 0;i<sizeof(cc);i++){  
        if(cc[i]!='\0') size++;
	}
	
	for (int i = 0;i<(size-1)/2;i++){
		
		num = static_cast<int>(cc[2*i]-'0'); 
		for (int j = 0;j<num;j++) {
			if (num>0) temp[k] = cc[2*i+1];
			k++;
			
			if(cc[2*i+1]=='a' || cc[2*i+1]=='A'||
			cc[2*i+1]=='e' || cc[2*i+1]=='E'||
			cc[2*i+1]=='i' || cc[2*i+1]=='I'||
			cc[2*i+1]=='o' || cc[2*i+1]=='O'||
			cc[2*i+1]=='u' || cc[2*i+1]=='U') vowel+=1;
			else consonant+=1;
		}

	}
	cout<<temp;
	cout<<endl;
	//cout<<vowel <<" "<<consonant<<endl;
	if (vowel>consonant) cout<<"Vowel > Consonant";
	else if (vowel==consonant) cout<<"Vowel = Consonant";
	else cout<<"Vowel < Consonant";

//TEMPLATE END

//APPEND BEGIN
return 0;
}
//APPEND END
